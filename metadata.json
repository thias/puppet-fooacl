{
  "name": "thias-fooacl",
  "version": "1.0.2",
  "author": "Matthias Saou",
  "license": "Apache-2.0",
  "summary": "POSIX filesystem ACLs module",
  "source": "https://github.com/thias/puppet-fooacl",
